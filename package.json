{
  "name": "leverex",
  "private": true,
  "scripts": {
    "start": "next start -p 80",
    "start:dev": "cross-env ENV=dev FE=$(git rev-parse --short HEAD) next dev -p 8080",
    "start:uat": "cross-env ENV=uat FE=$(git rev-parse --short HEAD) next dev -p 8080",
    "start:live": "cross-env ENV=live FE=$(git rev-parse --short HEAD) next dev -p 8080",
    "start:prod": "cross-env ENV=prod FE=$(git rev-parse --short HEAD) next dev -p 8080",
    "start:devprem": "cross-env ENV=devprem FE=$(git rev-parse --short HEAD) next dev -p 8080",
    "build": "next build",
    "build:dev": "cross-env ENV=dev FE=$(git rev-parse --short HEAD) next build",
    "build:uat": "cross-env ENV=uat FE=$(git rev-parse --short HEAD) next build && cross-env ENV=uat next-sitemap",
    "build:live": "cross-env ENV=live FE=$(git rev-parse --short HEAD) next build",
    "build:prod": "cross-env ENV=prod FE=$(git rev-parse --short HEAD) next build && cross-env ENV=prod next-sitemap",
    "build:devprem": "cross-env ENV=devprem FE=$(git rev-parse --short HEAD) next build && cross-env ENV=devprem next-sitemap",
    "prepare": "husky install",
    "lint:check": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --cache --ext .ts,.tsx --fix",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "posttest": "yarn format",
    "test:check": "cross-env ENV=test jest --silent",
    "test:watch": "cross-env ENV=test jest --watch",
    "test:coverage": "cross-env ENV=test jest --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "camelcase-keys": "7.0.2",
    "clsx": "1.1.1",
    "crypto-js": "4.1.1",
    "html2canvas": "1.4.1",
    "jspdf": "2.5.1",
    "jspdf-autotable": "3.5.25",
    "merkletreejs": "0.2.32",
    "next": "12.1.1",
    "parallax-js": "3.1.0",
    "react": "17.0.2",
    "react-datepicker": "4.7.0",
    "react-device-detect": "2.1.2",
    "react-dom": "17.0.2",
    "react-dropzone": "14.2.1",
    "react-qr-code": "2.0.4",
    "react-redux": "7.2.6",
    "react-swipeable": "7.0.0",
    "react-virtualized": "9.22.3",
    "redux-persist": "6.0.0",
    "redux-ws-middleware": "1.0.5",
    "snakecase-keys": "5.1.2",
    "typewriter-effect": "2.18.2"
  },
  "devDependencies": {
    "@svgr/webpack": "6.2.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@types/crypto-js": "4.1.1",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.17",
    "@types/react": "17.0.39",
    "@types/react-datepicker": "4.4.1",
    "@types/react-dom": "18.0.0",
    "@types/react-virtualized": "9.21.21",
    "@types/redux-logger": "3.0.9",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "autoprefixer": "10.4.2",
    "cross-env": "7.0.3",
    "dotenv": "16.0.0",
    "eslint": "8.9.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-unicorn": "40.1.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jira-prepare-commit-msg": "1.6.2",
    "next-compose-plugins": "2.2.1",
    "next-sitemap": "3.1.21",
    "postcss": "8.4.6",
    "prettier": "2.5.1",
    "redux-logger": "3.0.6",
    "tailwindcss": "3.0.22",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "$J $M"
  }
}
